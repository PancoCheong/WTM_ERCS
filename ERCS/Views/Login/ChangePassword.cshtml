@model ERCS.ViewModel.HomeVMs.ChangePasswordVM

<p style="text-align:center;margin-top:10px;font-size:16px">@Model.Localizer["ChangePassword"]</p>
<wt:form vm="@Model">
  <wt:textbox field="ITCode" disabled="true" />
  <wt:textbox is-password="true" field="OldPassword" />
  <wt:textbox is-password="true" field="NewPassword" />
  <wt:textbox is-password="true" field="NewPasswordComfirm" />
  <wt:row align="AlignEnum.Center">
    <wt:submitbutton size=" ButtonSizeEnum.Normal" />
  </wt:row>
</wt:form>
